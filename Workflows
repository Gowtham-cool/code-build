version: 0.2
phases:
  install:
    commands:
      - echo "Installing dependencies"
      - apt-get update -y
  build:
    commands:
      - echo "Building asset bundle"
      - zip -r output.zip ./assets
  post_build:
    commands:
      - echo "Uploading bundle to S3"
      - aws s3 cp output.zip s3://your-bucket-name/output.zip
artifacts:
  files:
    - output.zip

